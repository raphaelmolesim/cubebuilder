<h1>Cubes</h1>

<table class="table">
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @cubes.each do |cube| %>
      <tr>
        <td><%= cube.name %></td>
        <td><%= link_to 'Edit', edit_cube_path(cube) %></td>
        <td><%= link_to 'Delete', cube, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>