<p id="notice"><%= notice %></p>

<h1>Cubes</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Password</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @cubes.each do |cube| %>
      <tr>
        <td><%= cube.name %></td>
        <td><%= cube.password %></td>
        <td><%= link_to 'Show', cube %></td>
        <td><%= link_to 'Edit', edit_cube_path(cube) %></td>
        <td><%= link_to 'Destroy', cube, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Cube', new_cube_path %>
