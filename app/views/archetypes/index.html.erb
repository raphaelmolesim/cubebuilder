<p id="notice"><%= notice %></p>

<h1>Archetypes</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @archetypes.each do |archetype| %>
      <tr>
        <td><%= archetype.name %></td>
        <td><%= link_to 'Show', archetype %></td>
        <td><%= link_to 'Edit', edit_archetype_path(archetype) %></td>
        <td><%= link_to 'Destroy', archetype, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Archetype', new_archetype_path %>
